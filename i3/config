    bindsym $mod+slash exec python $SCRIPTS/mouse_jumper.py 18 13 && $SCRIPTS/i3_mouse_move_additional_action.sh
    bindsym $mod+shift+slash exec python $SCRIPTS/mouse_jumper.py 18 14 && $SCRIPTS/i3_mouse_move_additional_action.sh
    bindsym shift+slash exec python $SCRIPTS/mouse_jumper.py 18 15 && $SCRIPTS/i3_mouse_move_additional_action.sh
    bindsym Mod1+slash exec python $SCRIPTS/mouse_jumper.py 19 12 && $SCRIPTS/i3_mouse_move_additional_action.sh
    bindsym Mod1+$mod+slash exec python $SCRIPTS/mouse_jumper.py 19 13 && $SCRIPTS/i3_mouse_move_additional_action.sh
    bindsym Mod1+$mod+shift+slash exec python $SCRIPTS/mouse_jumper.py 19 14 && $SCRIPTS/i3_mouse_move_additional_action.sh
    bindsym Mod1+shift+slash exec python $SCRIPTS/mouse_jumper.py 19 15 && $SCRIPTS/i3_mouse_move_additional_action.sh


    # prev mode
    # bindsym Shift+space mode "default"; exec xdotool mouseup 1

    # next mode
    bindsym apostrophe mode "mode_small_mouse_move"; exec xdotool keydown super keyup super
    bindsym $mod+space mode "mode_small_mouse_move"; exec xdotool keydown super keyup super

    # # next next mode
    # bindsym Menu mode "mode_click"; exec xdotool keydown super keyup super && xdotool mouseup 1

    # exit mode
    bindsym $sup+space mode "default"; exec xdotool keydown super keyup super && xdotool mouseup 1 && gromit-mpx -v
    bindsym Escape mode "default"; exec xdotool keydown super keyup super && xdotool mouseup 1 && gromit-mpx -v

    # Left click and exit
    bindsym space mode "default"; exec xdotool click 1 && xdotool mouseup 1 && gromit-mpx -v

    # Right click and exit
    bindsym Shift+space mode "default"; exec xdotool click 3 && xdotool mouseup 1 && gromit-mpx -v

    # Middle Click and exit
    bindsym Return mode "default"; exec xdotool click 2 && xdotool mouseup 1 && gromit-mpx -v

    # Hold down
    bindsym Mod1+space exec xdotool click 1 && xdotool mousedown 1 && i3-msg mode "mode_mouse_move" && xdotool mousemove_relative --sync 5 5

    # Release
    bindsym Mod1+Shift+space exec xdotool mouseup 1
}

bindsym $mod+a mode "mode_mouse_move"; exec gromit-mpx -v

# Font for window titles. Will also be used by the bar unless a different font
# is used in the bar {} block below.
font pango:monospace 8

# This font is widely installed, provides lots of unicode glyphs, right-to-left
# text rendering and scalability on retina/hidpi displays (thanks to pango).
#font pango:DejaVu Sans Mono 8

# Before i3 v4.8, we used to recommend this one as the default:
# font -misc-fixed-medium-r-normal--13-120-75-75-C-70-iso10646-1
# The font above is very space-efficient, that is, it looks good, sharp and
# clear in small sizes. However, its unicode glyph coverage is limited, the old
# X core fonts rendering does not support right-to-left and this being a bitmap
# font, it doesnt scale on retina/hidpi displays.

# Use Mouse+$mod to drag floating windows to their wanted position
floating_modifier $mod

# start a terminal
