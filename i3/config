# LOCKS
bindsym $sup+l [con_mark="^Chromium-browser$"] focus; [con_mark="^Gnome-terminal$"] focus; exec --no-startup-id systemctl suspend && ~/.config/i3/scripts/i3_locking.sh
# bindsym ctrl+$mod+l exec --no-startup-id sleep 1 && xset dpms force suspend

# exec xautolock -time 15 -locker 
exec xautolock -time 15 -locker "~/.config/i3/scripts/i3_locking.sh" -notify 20 -notifier 'xset dpms force off'
# xautolock -time 7 -locker "systemctl suspend" &

# https://www.reddit.com/r/i3wm/comments/9ebemt/locking_i3_when_lid_of_laptop_is_closed/
exec --no-startup-id xss-lock -- ~/.config/i3/scripts/i3_locking.sh

# redshift (like f.lux)
exec redshift -t 4000:4000

# creates mode for shutdown, suspend, hibernate, reboot, shutdown  
set $locker ~/.config/i3/i3lock-wrapper -l && sleep 1

set $mode_system
set $mode_system system (l) lock, (e) logout, (s) suspend, (h) hibernate, (r) reboot, (shift+s) shutdown
mode "$mode_system" {
        bindsym l exec --no-startup-id $locker, mode "default"
                bindsym e exec --no-startup-id i3-msg exit, mode "default"
                    bindsym s exec --no-startup-id $locker && systemctl suspend, mode "default"
                        bindsym h exec --no-startup-id $locker && systemctl hibernate, mode "default"
                            bindsym r exec --no-startup-id systemctl reboot, mode "default"
                                bindsym shift+s exec --no-startup-id  systemctl poweroff -i, mode "default"

                                    # back to normal: enter or escape
                                    bindsym Return mode "default"
                                    bindsym Escape mode "default"
                                    bindsym $sup+space mode "default"
                                    bindsym m mode "mode_movement"
}

bindsym $sup+s mode "$mode_system"

# split in horizontal orientation
bindsym $mod+semicolon split h

# split in vertical orientation
bindsym $mod+v split v

# # enter fullscreen mode for the focused container
# bindsym $mod+f fullscreen toggle 

# global fullscreen: across all monitors
bindsym $mod+shift+f        fullscreen toggle global

# fullscreens the youtube video in a page
# i3-msg focusing is so that 'fullscreen is not available' disappears
bindsym $sup+f $fullscreenYoutube

set $mode_fullscreen
mode "mode_fullscreen" {
    bindsym $sup+l mode "default"; fullscreen disable; [con_mark="^Chromium-browser$"] focus; [con_mark="^Gnome-terminal$"] focus; exec amixer -D pulse sset Master mute && ~/.config/i3/scripts/i3_locking.sh && pkill -RTMIN+10 i3blocks

    # bindsym $mod+a mode "mode_mouse_move"
    bindsym Mode_switch+f mode "mode_mouse_move"; $mouseHintsVisibilityToggle && $SCRIPTS/auto_fullscreen_mode.sh
    bindsym Mode_switch+Shift+f mode "mode_common_clicks"; exec $SCRIPTS/auto_fullscreen_mode.sh

    # bar mode toggle
    bindsym $mod+f fullscreen disable; mode "default"; bar mode hide

    bindsym $mod+l fullscreen disable; focus right; fullscreen enable
    bindsym $mod+h fullscreen disable; focus left; fullscreen enable
    bindsym $mod+k fullscreen disable; focus up; fullscreen enable
    bindsym $mod+j fullscreen disable; focus down; fullscreen enable

    bindsym $mod+Shift+l mode "default"; fullscreen toggle; focus right
    bindsym $mod+Shift+h mode "default"; fullscreen disable; focus left
    bindsym $mod+Shift+k mode "default"; fullscreen disable; focus up
    bindsym $mod+Shift+j mode "default"; fullscreen disable; focus down

    # reload the configuration file
    bindsym $mod+shift+c reload; bar mode invisible

    # switch to workspace
    bindsym $sup+1 fullscreen disable; workspace $workspace1; fullscreen enable; bar mode hide; 
    bindsym $sup+2 fullscreen disable; workspace $workspace2; fullscreen enable; bar mode hide; 
    bindsym $sup+3 fullscreen disable; workspace $workspace3; fullscreen enable; bar mode hide; 
    bindsym $sup+4 fullscreen disable; workspace $workspace4; fullscreen enable; bar mode hide; 
    bindsym $sup+5 fullscreen disable; workspace $workspace5; fullscreen enable; bar mode hide; 
    bindsym $sup+6 fullscreen disable; workspace $workspace6; fullscreen enable; bar mode hide; 
    bindsym $sup+7 fullscreen disable; workspace $workspace7; fullscreen enable; bar mode hide; 
    bindsym $sup+8 fullscreen disable; workspace $workspace8; fullscreen enable; bar mode hide; 
    bindsym $sup+9 fullscreen disable; workspace $workspace9; fullscreen enable; bar mode hide; 
    bindsym $sup+0 fullscreen disable; workspace $workspace10; fullscreen enable; bar mode hide; 

    # Copied from default mode
    bindsym $mod+shift+q kill

    # printscreen
    bindsym Print --release exec --no-startup-id $SCRIPTS/screenshots.sh 0
    bindsym Shift+Print exec --no-startup-id $SCRIPTS/screenshots.sh 1
    bindsym Shift+control+Print exec --no-startup-id scrot -s /tmp/clipped.png && xclip -selection c -t image/png /tmp/clipped.png

    # increase/decrease keyboard brightness
    bindsym XF86KbdBrightnessUp $keyboardLightUp
    bindsym XF86KbdBrightnessDown $keyboardLightDown

    # Touchpad controls
    bindsym XF86TouchpadToggle $toggleTouchpad

    # Pulse Audio controls
    #increase sound volume
    bindsym XF86AudioRaiseVolume $volumeRaise
#decrease sound volume
    bindsym XF86AudioLowerVolume $volumeLower
# mute sound
    bindsym XF86AudioMute $volumeMute

    # Screen brightness controls with notification
    # increase screen brightness with notification
    bindsym XF86MonBrightnessUp $brightnessUp
    # decrease screen brightness with notification
    bindsym XF86MonBrightnessDown $brightnessDown
    bindsym $mod+z $brightnessDown
    bindsym $mod+Shift+z $brightnessMinimal
    bindsym $mod+x $brightnessUp

    # When taking notes, can do this
    bindsym $mod+a $brightnessFlashingNoteMode

    # restart i3 inplace (preserves your layout/session, can be used to upgrade i3)
    bindsym $mod+shift+r restart

    # start dmenu (a program launcher)
    # bindsym $mod+d exec dmenu_run -nb black -sb tomato -l 10
    # bindsym $mod+d exec rofi -show run -lines 7 -eh 2 -bg "black" -fg "#B00000" -font "System San Francisco Display 15" -hlfg "#999999"
    bindsym $mod+d $rofi
    bindsym $mod+shift+d $rofiWindow

    # Tab movement
    bindsym Mod1+Tab fullscreen disable; $altTabNext && i3-msg "fullscreen enable" && xdotool keydown super keyup super
    bindsym Mod1+Shift+Tab fullscreen disable; $altTabPrev && i3-msg "fullscreen enable" && xdotool keydown super keyup super
    bindsym Mode_switch+a fullscreen disable; exec $altTabNext && i3-msg "fullscreen enable" && xdotool keydown super keyup super
    bindsym Mode_switch+Shift+a fullscreen disable; exec $altTabPrev && i3-msg "fullscreen enable" && xdotool keydown super keyup super

    bindsym $sup+Tab fullscreen disable; workspace back_and_forth; exec i3-msg "fullscreen enable" && xdotool keydown super keyup super

    # [A] Alternate between the two most recently used containers/windows
    bindsym Mode_switch+s fullscreen disable; workspace back_and_forth; exec i3-msg "fullscreen enable" && xdotool keydown super keyup super

    bindsym $sup+Shift+Tab fullscreen disable; move container to workspace back_and_forth; workspace back_and_forth; exec i3-msg "fullscreen enable" && xdotool keydown super keyup super
    bindsym Mode_switch+Shift+s fullscreen disable; move container to workspace back_and_forth; workspace back_and_forth; exec i3-msg "fullscreen enable" && xdotool keydown super keyup super

    bindsym $sup+grave fullscreen disable; workspace next; exec i3-msg "fullscreen enable" && xdotool keydown super keyup super
    bindsym $sup+Shift+grave fullscreen disable; workspace prev; exec i3-msg "fullscreen enable" && xdotool keydown super keyup super
    bindsym Mode_switch+w fullscreen disable; workspace next; exec i3-msg "fullscreen enable" && xdotool keydown super keyup super
    bindsym Mode_switch+Shift+w fullscreen disable; workspace prev; exec i3-msg "fullscreen enable" && xdotool keydown super keyup super

    bindsym Mod1+grave fullscreen disable; exec python3 $SCRIPTS/switch_windows_by_class.py 0; exec i3-msg "fullscreen enable" && xdotool keydown super keyup super
    bindsym Mod1+Shift+grave fullscreen disable; exec python3 $SCRIPTS/switch_windows_by_class.py 1; exec i3-msg "fullscreen enable" && xdotool keydown super keyup super

    # [S]witch focus fullscreen disable; between windows by class; mode "default"
    bindsym Mod1+Mode_switch+s fullscreen disable; exec python3 $SCRIPTS/switch_windows_by_class.py 0 && i3-msg "fullscreen enable" && xdotool keydown super keyup super
    bindsym Mod1+Mode_switch+Shift+s fullscreen disable; exec python3 $SCRIPTS/switch_windows_by_class.py 1 && exec i3-msg "fullscreen enable" && xdotool keydown super keyup super

    bindsym Mode_switch+grave fullscreen disable; exec $SCRIPTS/move_to_next_workspace.sh next; exec i3-msg "fullscreen enable" && xdotool keydown super keyup super
    bindsym Mode_switch+Shift+grave fullscreen disable; exec $SCRIPTS/move_to_next_workspace.sh prev; exec i3-msg "fullscreen enable" && xdotool keydown super keyup super

    # Scrolling
    bindsym Mod1+j $scrollDown
    bindsym Mod1+k $scrollUp

    # Clicking
    bindsym $sup+j $left_mouse_click
    bindsym $sup+Shift+j $right_mouse_click
    bindsym $sup+k $middle_mouse_click

    # [D]o often (switches between two most often used windows: chromium and terminal)
    # bindsym Mode_switch+g fullscreen disable
    bindsym Mode_switch+g fullscreen disable; exec $SCRIPTS/switch_between_chromium_terminal.sh 0 && i3-msg "fullscreen enable" && xdotool keydown super keyup super
    bindsym Mode_switch+Shift+g fullscreen disable; exec $SCRIPTS/switch_between_chromium_terminal.sh 1 && i3-msg "fullscreen enable" && xdotool keydown super keyup super

    bindsym Mode_switch+$mark1 fullscreen disable; exec $SCRIPTS/jumpmarks_or_scratchpad.sh $mark1 && i3-msg "fullscreen enable" && xdotool keydown super keyup super
    bindsym Mode_switch+$mark2 fullscreen disable; exec $SCRIPTS/jumpmarks_or_scratchpad.sh $mark2 && i3-msg "fullscreen enable" && xdotool keydown super keyup super
    bindsym Mode_switch+$mark3 fullscreen disable; exec $SCRIPTS/jumpmarks_or_scratchpad.sh $mark3 && i3-msg "fullscreen enable" && xdotool keydown super keyup super
    # # 53 is x
    bindcode Mode_switch+Mod1+53 [con_id="__focused__"] unmark 

    # # Jump marks
    bindsym Mode_switch+Shift+$mark1 fullscreen disable; exec $SCRIPTS/move_to_mark_and_focus.sh $mark1 && i3-msg "fullscreen enable" && xdotool keydown super keyup super
    bindsym Mode_switch+Shift+$mark2 fullscreen disable; exec $SCRIPTS/move_to_mark_and_focus.sh $mark2 && i3-msg "fullscreen enable" && xdotool keydown super keyup super
    bindsym Mode_switch+Shift+$mark3 fullscreen disable; exec $SCRIPTS/move_to_mark_and_focus.sh $mark3 && i3-msg "fullscreen enable" && xdotool keydown super keyup super

    bindsym Mode_switch+Shift+$mark_term fullscreen disable; exec $SCRIPTS/move_to_mark_and_focus.sh $mark_term_name && i3-msg "fullscreen enable" && xdotool keydown super keyup super
    bindsym Mode_switch+Shift+$mark_browser3 fullscreen disable; exec $SCRIPTS/move_to_mark_and_focus.sh $mark_browser_name3 && i3-msg "fullscreen enable" && xdotool keydown super keyup super
    bindsym Mode_switch+Shift+$mark_browser1 fullscreen disable; exec $SCRIPTS/move_to_mark_and_focus.sh $mark_browser_name1 && i3-msg "fullscreen enable" && xdotool keydown super keyup super
    bindsym Mode_switch+Shift+$mark_browser2 fullscreen disable; exec $SCRIPTS/move_to_mark_and_focus.sh $mark_browser_name2 && i3-msg "fullscreen enable" && xdotool keydown super keyup super
    bindsym Mode_switch+Shift+$mark_pdf_viewer fullscreen disable; exec $SCRIPTS/move_to_mark_and_focus.sh $mark_pdf_viewer_name && i3-msg "fullscreen enable" && xdotool keydown super keyup super

    bindsym Mode_switch+$mark_term fullscreen disable; exec $SCRIPTS/jumpmarks_or_scratchpad.sh $mark_term_name && i3-msg "fullscreen enable" && xdotool keydown super keyup super
    bindsym Mode_switch+$mark_browser3 fullscreen disable; exec $SCRIPTS/jumpmarks_or_scratchpad.sh $mark_browser_name3 && i3-msg "fullscreen enable" && xdotool keydown super keyup super
    bindsym Mode_switch+$mark_browser1 fullscreen disable; exec $SCRIPTS/jumpmarks_or_scratchpad.sh $mark_browser_name1 && i3-msg "fullscreen enable" && xdotool keydown super keyup super
    bindsym Mode_switch+$mark_browser2 fullscreen disable; exec $SCRIPTS/jumpmarks_or_scratchpad.sh $mark_browser_name2 && i3-msg "fullscreen enable" && xdotool keydown super keyup super
    bindsym Mode_switch+$mark_pdf_viewer fullscreen disable; exec $SCRIPTS/jumpmarks_or_scratchpad.sh $mark_pdf_viewer_name && i3-msg "fullscreen enable" && xdotool keydown super keyup super
}
# xdotool used to remove the obstructive status bar up top until super is pressed again
bindsym $mod+f mode "mode_fullscreen"; fullscreen enable; exec xdotool keydown super keyup super
# bindsym $mod+f fullscreen toggle

# change container layout (stacked, tabbed, toggle split)
bindsym $mod+s layout stacking
bindsym $mod+w layout tabbed
bindsym $mod+e layout toggle split

# toggle sticky from floating
bindsym $mod+$sup+space sticky toggle

# toggle tiling / floating
bindsym $sup+space floating toggle; sticky disable

# change focus between tiling / floating windows
bindsym $mod+space focus mode_toggle

# move to left:
bindsym $mod+Mod1+h  exec $SCRIPTS/i3-move-resize.py -2 -1 -1 -1

#move to right:
bindsym $mod+Mod1+l  exec $SCRIPTS/i3-move-resize.py -4 -1 -1 -1

#move up:
bindsym $mod+Mod1+k exec $SCRIPTS/i3-move-resize.py -1 -2 -1 -1

#move down:
bindsym $mod+Mod1+j  exec $SCRIPTS/i3-move-resize.py -1 -4 -1 -1

# focus the parent container
bindsym $sup+a focus parent

# focus the child container
bindsym $sup+q focus child

set $workspace1      "1 T"
set $workspace2      "2 "
set $workspace3      "3"
set $workspace4      "4"
set $workspace5      "5"
set $workspace6      "6"
set $workspace7      "7"
set $workspace8      "8"
