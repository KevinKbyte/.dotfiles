# This file has been auto-generated by i3-config-wizard(1).
# It will not be overwritten, so edit it as you like.
#
# Should you change your keyboard layout some time, delete
# this file and re-run i3-config-wizard(1).

# i3 config file (v4)
#
# Please see http://i3wm.org/docs/userguide.html for a complete reference!

set $SCRIPTS $HOME/.dotfiles/i3/scripts
set $term --no-startup-id gnome-terminal -- /bin/bash -c
set $xterm --no-startup-id xterm -fa monaco -fs 13

# Mod4 is super key
set $sup Mod4
# Mod3 is caps lock is Hyper_L (caps lock for me)
set $mod Mod3

set $screenSize 1024x768

# to connect w/ HDMI cable
bindsym F8 exec --no-startup-id $SCRIPTS/hdmi.sh
# 1920 x 1080,
# https://i3wm.org/docs/userguide.html (i3 and the rest of your software world)
# Case 2: You can see more than your audience
bindsym Shift+F8 exec xrandr --output VGA1 --mode $screenSize --right-of LVDS1
# Case 1: everybody gets the same output
bindsym $mod+F8 exec xrandr --output VGA1 --mode $screenSize --same-as LVDS1

# printscreen
bindsym --release Shift+Print exec --no-startup-id $SCRIPTS/screenshots.sh 0
bindsym --release Print exec --no-startup-id $SCRIPTS/screenshots.sh 1
bindsym --release Shift+control+Print exec --no-startup-id scrot -s /tmp/clipped.png && xclip -selection c -t image/png /tmp/clipped.png
 
# increase/decrease keyboard brightness
bindsym XF86KbdBrightnessUp exec $SCRIPTS/kbd_backlight.sh up
bindsym XF86KbdBrightnessDown exec $SCRIPTS/kbd_backlight.sh down 

# Touchpad controls
# bindsym XF86TouchpadToggle exec $SCRIPTS/toggle_touchpad.sh # toggle touchpad
bindsym XF86TouchpadToggle exec $SCRIPTS/toggle_touchpad.sh # toggle touchpad

# Pulse Audio controls
# Taken from https://www.youtube.com/watch?v=gKumet6b-WY (~4:04)
# RTMIN+10 means that it kills command with signal 10, which is the volume
#increase sound volume
bindsym XF86AudioRaiseVolume exec "amixer -D pulse sset Master 5%+; notify-send 'volume up' && pkill -RTMIN+10 i3blocks"
#decrease sound volume
bindsym XF86AudioLowerVolume exec "amixer -D pulse sset Master 5%-; notify-send 'volume down' && pkill -RTMIN+10 i3blocks"
# mute sound
bindsym XF86AudioMute exec "amixer -D pulse sset Master toggle; notify-send 'volume toggled' && pkill -RTMIN+10 i3blocks"

# Screen brightness controls with notification
# increase screen brightness with notification
bindsym XF86MonBrightnessUp exec "light -A 5; notify-send 'brightness up'"
# decrease screen brightness with notification
bindsym XF86MonBrightnessDown exec "light -U 5; notify-send 'brightness down'"
bindsym $mod+z exec light -U 10
bindsym $mod+x exec light -A 5 

bindsym Mode_switch+r exec $xterm ranger 

# Common clicks
bindsym $mod+shift+a mode "mode_common_clicks"

mode "mode_common_clicks" {
set $autoplayX 1826
set $autoplayY 182
set $autoplaySleepOGPosition 0.075

set $backPageX 17
set $backPageY 61
set $backSleepOGPosition 0.1

set $pluginX 1264
set $pluginY 278
set $pluginSleepWaitForKeyUp 0.2
set $pluginSleepLoadSettingsPage 0.4
set $pluginSleepOGPosition 0.4

set $downX 1911
set $downY 1057

set $upX 1919
set $upY 106

set $timeX 1050
set $timeY 100

    # Click Youtube Autoplay
    bindsym a mode "default"; exec eval $(xdotool getmouselocation --shell) && xdotool mousemove $autoplayX $autoplayY && xdotool click 1 && sleep $autoplaySleepOGPosition && xdotool mousemove $X $Y
    
    # Opens previous page in new tab
    bindsym b mode "default"; exec eval $(xdotool getmouselocation --shell) && xdotool mousemove $backPageX $backPageY && xdotool click 2 && sleep $backSleepOGPosition && xdotool mousemove $X $Y

    # Toggles plugin
    bindsym s mode "default" exec sleep $pluginSleepWaitForKeyUp && xdotool key Control_L+l && xdotool key s && xdotool key Alt_R+Return && xdotool key Control_L+l && xdotool key s+Return && eval $(xdotool getmouselocation --shell) && sleep $pluginSleepLoadSettingsPage && xdotool mousemove $pluginX $pluginY && xdotool click 1 && sleep $pluginSleepOGPosition && xdotool mousemove $X $Y && xdotool key Control_L+w

    # Moves up/down the page by left mousing the scroll bar
    bindsym j mode "mode_mouse_move"; exec xdotool mousemove $downX $downY && xdotool mousedown 1
    bindsym k mode "mode_mouse_move"; exec xdotool mousemove $upX $upY && xdotool mousedown 1

    # Shows Time and Battery
    bindsym t mode "default"; exec gnome-terminal --window-with-profile=Increase -- /bin/bash -c '$SCRIPTS/terminal_time.sh' && i3-msg floating toggle && i3-msg resize set $timeX $timeY && i3-msg move absolute position center

    # Shows Weather
    bindsym w mode "default"; exec $term '$SCRIPTS/i3weatherterminal.sh' 

    bindsym $sup+space mode "default"
    bindsym space mode "mode_mouse_move"
}

