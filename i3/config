# locks
bindsym $sup+l [con_mark="^Chromium-browser$"] focus; [con_mark="^Gnome-terminal$"] focus; exec amixer -D pulse sset Master mute && ~/.config/i3/scripts/i3_locking.sh && pkill -RTMIN+10 i3blocks
# exec xautolock -time 15 -locker 
