# Should you change your keyboard layout some time, delete
# this file and re-run i3-config-wizard(1).

# i3 config file (v4)
#
# Please see http://i3wm.org/docs/userguide.html for a complete reference!

set $SCRIPTS /home/$USER/.dotfiles/i3/scripts
set $term --no-startup-id gnome-terminal -- /bin/bash -c
set $xterm --no-startup-id xterm -fa monaco -fs 13
# set $cursor_hints $HOME/test_files/

# Mod4 is super key
# set $sup Mod4
set $sup Mod4
# Mod3 is caps lock is Hyper_L (caps lock for me)
set $mod Mod3
set $screenSize 1024x768
set $connectWithHDMI exec --no-startup-id ~/.dotfiles/i3/scripts/hdmi.sh
set $connectWithHDMISpecificScreen exec xrandr --output VGA1 --mode $screenSize --right-of LVDS1
set $connectWithHDMISharedScreens  exec xrandr --output VGA1 --mode $screenSize --same-as LVDS1

set $printscreen exec --no-startup-id ~/.dotfiles/i3/scripts/screenshots.sh 1
set $printscreenWithSelect --release exec ~/.dotfiles/i3/scripts/screenshots.sh 0
set $printscreenSelectAndCopy exec --no-startup-id scrot -s /tmp/clipped.png && xclip -selection c -t image/png /tmp/clipped.png

set $keyboardLightUp exec ~/.dotfiles/i3/scripts/kbd_backlight.sh up
set $keyboardLightDown exec ~/.dotfiles/i3/scripts/kbd_backlight.sh down 

set $toggleTouchpad exec /home/$USER/.dotfiles/i3/scripts/toggle_touchpad.sh 
set $volumeLower exec "amixer -D pulse sset Master 5%- && notify-send 'volume down' && pkill -RTMIN+10 i3blocks"
set $volumeRaise exec "amixer -D pulse sset Master 5%+ && notify-send 'volume up' && pkill -RTMIN+10 i3blocks"
set $volumeMute exec "amixer -D pulse sset Master toggle && notify-send 'volume toggled' && pkill -RTMIN+10 i3blocks"

set $brightnessUp exec "light -A 5"
set $brightnessDown exec "light -U 10"
set $brightnessMinimal exec light -S 0.1
set $brightnessFlashingNoteMode exec light -S 5 && sleep 3 && light -S 0
set $rangerFileExplorer exec $xterm ranger 

set $modeCommonClicks mode "mode_common_clicks"
set $modeMouseMove mode "mode_mouse_move"
set $mouseHintsVisibilityToggle exec gromit-mpx -v

set $scrollDown exec xdotool click 5 click 5 click 5
set $scrollUp exec xdotool click 4 click 4 click 4

set $middle_mouse_click exec xdotool click 2
set $left_mouse_click exec xdotool click 1
set $right_mouse_click exec xdotool click 3

set $rofi exec rofi -show run -lines 7 -eh 2 -theme $HOME/.dotfiles/i3/rofi-themes/Arc-Dark.rasi
set $rofiWindow exec rofi -theme $HOME/.dotfiles/i3/rofi-themes/Arc-Dark.rasi -show 
set $appWeather exec --no-startup-id gnome-terminal -- /bin/bash -c '~/.dotfiles/i3/scripts/i3weatherterminal.sh' 
set $appTimeBattery exec gnome-terminal --window-with-profile=Increase -- /bin/bash -c '~/.dotfiles/i3/scripts/terminal_time.sh' && i3-msg floating toggle && i3-msg resize set $timeX $timeY && i3-msg move absolute position center
set $fullscreenYoutube exec sleep 0.2 && xdotool keydown f keyup f && sleep 0.2 && i3-msg fullscreen enable && i3-msg fullscreen disable && i3-msg && sleep 2.5 && i3-msg focus floating && i3-msg focus tiling

set $altTabNext exec ~/.dotfiles/i3/scripts/alt_tab.sh next 
set $altTabPrev exec ~/.dotfiles/i3/scripts/alt_tab.sh prev
set $switchWindowsByClassNext exec python3 ~/.dotfiles/i3/scripts/switch_windows_by_class.py 0
set $switchWindowsByClassPrev exec python3 ~/.dotfiles/i3/scripts/switch_windows_by_class.py 1


# to connect w/ HDMI cable
bindsym F8 $connectWithHDMI
# 1920 x 1080,
# https://i3wm.org/docs/userguide.html (i3 and the rest of your software world)
# Case 2: You can see more than your audience
bindsym Shift+F8 $connectWithHDMISpecificScreen
# Case 1: everybody gets the same output
bindsym $mod+F8 $connectWithHDMISharedScreens

# printscreen
# https://www.reddit.com/r/i3wm/comments/9hh4jl/i3_wont_execute_the_imagemagick_import_command/
bindsym Print $printscreenWithSelect
bindsym Shift+Print $printscreen
bindsym Shift+control+Print $printscreenSelectAndCopy
 
# increase/decrease keyboard brightness
bindsym XF86KbdBrightnessUp $keyboardLightUp
bindsym XF86KbdBrightnessDown $keyboardLightDown

# Touchpad controls
# bindsym XF86TouchpadToggle exec $SCRIPTS/toggle_touchpad.sh # toggle touchpad
bindsym XF86TouchpadToggle $toggleTouchpad

# Pulse Audio controls
# Taken from https://www.youtube.com/watch?v=gKumet6b-WY (~4:04)
# RTMIN+10 means that it kills command with signal 10, which is the volume
#increase sound volume
bindsym XF86AudioRaiseVolume $volumeRaise
#decrease sound volume
bindsym XF86AudioLowerVolume $volumeLower
# mute sound
bindsym XF86AudioMute $volumeMute

# Screen brightness controls with notification
# increase screen brightness with notification
bindsym XF86MonBrightnessUp $brightnessUp
# decrease screen brightness with notification
bindsym XF86MonBrightnessDown $brightnessDown
bindsym $mod+z $brightnessDown
bindsym $mod+Shift+z $brightnessMinimal
bindsym $mod+x $brightnessUp

# When taking notes, can do this
bindsym $mod+a $brightnessFlashingNoteMode

bindsym Mode_switch+r $rangerFileExplorer

# Common clicks
# bindsym $mod+Shift+a mode "mode_common_clicks"
bindsym Mode_switch+Shift+f $modeCommonClicks

mode "mode_common_clicks" {
    set $autoplayX 1826
    set $autoplayY 182
    set $autoplaySleepOGPosition 0.075

    set $backPageX 17
    set $backPageY 61
    set $backSleepOGPosition 0.1

    set $pluginX 1264
    set $pluginY 278
    set $pluginSleepWaitForKeyUp 0.2
    set $pluginSleepLoadSettingsPage 0.4
    set $pluginSleepOGPosition 0.4

    set $downX 1911
    set $downY 1057

    set $upX 1919
    set $upY 106

    set $timeX 1050
    set $timeY 100

    # Click Youtube Autoplay
    bindsym a mode "default"; exec eval $(xdotool getmouselocation --shell) && xdotool mousemove $autoplayX $autoplayY && xdotool click 1 && sleep $autoplaySleepOGPosition && xdotool mousemove $X $Y
    
    # Opens previous page in new tab
    bindsym b mode "default"; exec eval $(xdotool getmouselocation --shell) && xdotool mousemove $backPageX $backPageY && xdotool click 2 && sleep $backSleepOGPosition && xdotool mousemove $X $Y

    # Toggles plugin
    bindsym s mode "default" exec sleep $pluginSleepWaitForKeyUp && xdotool key Control_L+l && xdotool key s && xdotool key Alt_R+Return && xdotool key Control_L+l && xdotool key s+Return && eval $(xdotool getmouselocation --shell) && sleep $pluginSleepLoadSettingsPage && xdotool mousemove $pluginX $pluginY && xdotool click 1 && sleep $pluginSleepOGPosition && xdotool mousemove $X $Y && xdotool key Control_L+w

    # Moves up/down the page by left mousing the scroll bar
    bindsym j mode "mode_mouse_move"; exec xdotool mousemove $downX $downY && xdotool mousedown 1 && gromit-mpx -v
    bindsym k mode "mode_mouse_move"; exec xdotool mousemove $upX $upY && xdotool mousedown 1 && gromit-mpx -v

    # Shows Time and Battery
    bindsym t mode "default"; $appTimeBattery

    # Shows Weather
    bindsym w mode "default"; $appWeather

    bindsym $sup+space mode "default"
    bindsym space mode "mode_mouse_move"; $mouseHintsVisibilityToggle
}

