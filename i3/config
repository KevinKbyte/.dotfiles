    # bindsym $sup+Mod1+shift+z $mouseJumpScript 1 15 && $mouseMoveAdditionalActionScriptClick

    bindsym $sup+x $mouseJumpScript 2 12 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+$mod+x $mouseJumpScript 2 13 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+$mod+shift+x $mouseJumpScript 2 14 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+shift+x $mouseJumpScript 2 15 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+x $mouseJumpScript 3 12 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+$mod+x $mouseJumpScript 3 13 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+$mod+shift+x $mouseJumpScript 3 14 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+shift+x $mouseJumpScript 3 15 && $mouseMoveAdditionalActionScriptClick

    bindsym $sup+c $mouseJumpScript 4 12 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+$mod+c $mouseJumpScript 4 13 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+$mod+shift+c $mouseJumpScript 4 14 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+shift+c $mouseJumpScript 4 15 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+c $mouseJumpScript 5 12 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+$mod+c $mouseJumpScript 5 13 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+$mod+shift+c $mouseJumpScript 5 14 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+shift+c $mouseJumpScript 5 15 && $mouseMoveAdditionalActionScriptClick

    bindsym $sup+v $mouseJumpScript 6 12 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+$mod+v $mouseJumpScript 6 13 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+$mod+shift+v $mouseJumpScript 6 14 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+shift+v $mouseJumpScript 6 15 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+v $mouseJumpScript 7 12 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+$mod+v $mouseJumpScript 7 13 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+$mod+shift+v $mouseJumpScript 7 14 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+shift+v $mouseJumpScript 7 15 && $mouseMoveAdditionalActionScriptClick

    bindsym $sup+b $mouseJumpScript 8 12 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+$mod+b $mouseJumpScript 8 13 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+$mod+shift+b $mouseJumpScript 8 14 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+shift+b $mouseJumpScript 8 15 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+b $mouseJumpScript 9 12 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+$mod+b $mouseJumpScript 9 13 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+$mod+shift+b $mouseJumpScript 9 14 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+shift+b $mouseJumpScript 9 15 && $mouseMoveAdditionalActionScriptClick

    bindsym $sup+n $mouseJumpScript 10 12 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+$mod+n $mouseJumpScript 10 13 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+$mod+shift+n $mouseJumpScript 10 14 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+shift+n $mouseJumpScript 10 15 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+n $mouseJumpScript 11 12 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+$mod+n $mouseJumpScript 11 13 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+$mod+shift+n $mouseJumpScript 11 14 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+shift+n $mouseJumpScript 11 15 && $mouseMoveAdditionalActionScriptClick

    bindsym $sup+m $mouseJumpScript 12 12 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+$mod+m $mouseJumpScript 12 13 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+$mod+shift+m $mouseJumpScript 12 14 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+shift+m $mouseJumpScript 12 15 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+m $mouseJumpScript 13 12 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+$mod+m $mouseJumpScript 13 13 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+$mod+shift+m $mouseJumpScript 13 14 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+shift+m $mouseJumpScript 13 15 && $mouseMoveAdditionalActionScriptClick

    bindsym $sup+comma $mouseJumpScript 14 12 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+$mod+comma $mouseJumpScript 14 13 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+$mod+shift+comma $mouseJumpScript 14 14 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+shift+comma $mouseJumpScript 14 15 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+comma $mouseJumpScript 15 12 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+$mod+comma $mouseJumpScript 15 13 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+$mod+shift+comma $mouseJumpScript 15 14 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+shift+comma $mouseJumpScript 15 15 && $mouseMoveAdditionalActionScriptClick

    bindsym $sup+period $mouseJumpScript 16 12 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+$mod+period $mouseJumpScript 16 13 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+$mod+shift+period $mouseJumpScript 16 14 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+shift+period $mouseJumpScript 16 15 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+period $mouseJumpScript 17 12 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+$mod+period $mouseJumpScript 17 13 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+$mod+shift+period $mouseJumpScript 17 14 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+shift+period $mouseJumpScript 17 15 && $mouseMoveAdditionalActionScriptClick

    bindsym $sup+slash $mouseJumpScript 18 12 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+$mod+slash $mouseJumpScript 18 13 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+$mod+shift+slash $mouseJumpScript 18 14 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+shift+slash $mouseJumpScript 18 15 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+slash $mouseJumpScript 19 12 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+$mod+slash $mouseJumpScript 19 13 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+$mod+shift+slash $mouseJumpScript 19 14 && $mouseMoveAdditionalActionScriptClick
    # bindsym $sup+Mod1+shift+slash $mouseJumpScript 19 15 && $mouseMoveAdditionalActionScriptClick


    # prev mode
    # bindsym Shift+space mode "default"; exec xdotool mouseup 1

    # next mode
    bindsym apostrophe mode "mode_small_mouse_move"; exec xdotool keydown super keyup super
    bindsym $mod+space mode "mode_small_mouse_move"; exec xdotool keydown super keyup super
    bindsym Mode_switch+f mode "mode_small_mouse_move"; exec xdotool keydown super keyup super

    # # next next mode
    # bindsym Menu mode "mode_click"; exec xdotool keydown super keyup super && xdotool mouseup 1

    # exit mode
    bindsym $sup+space mode "default"; exec xdotool keydown super keyup super && xdotool mouseup 1 && gromit-mpx -v
    bindsym Escape mode "default"; exec xdotool keydown super keyup super && xdotool mouseup 1 && gromit-mpx -v
    bindsym Menu mode "default"; exec xdotool keydown super keyup super && xdotool mouseup 1 && gromit-mpx -v
    # Mode_switch + e
    bindcode Mode_switch+26 mode "default"; exec xdotool keydown super keyup super && xdotool mouseup 1 && gromit-mpx -v
    bindcode Mode_switch+65 mode "default"; exec xdotool keydown super keyup super && xdotool mouseup 1 && gromit-mpx -v

    # Left click and exit
    bindsym space mode "default"; exec xdotool click 1 && xdotool mouseup 1 && gromit-mpx -v

    # Right click and exit
    bindsym Shift+space mode "default"; exec xdotool click 3 && xdotool mouseup 1 && gromit-mpx -v

    # Middle Click and exit
    bindsym Return mode "default"; exec xdotool click 2 && xdotool mouseup 1 && gromit-mpx -v

    # Hold down
    bindsym Mod1+space exec xdotool click 1 && xdotool mousedown 1 && i3-msg mode "mode_mouse_move" && xdotool mousemove_relative --sync 0.1 0.1

    # Release
    bindsym Mod1+Shift+space exec xdotool mouseup 1
}

# bindsym $mod+a mode "mode_mouse_move"; $mouseHintsVisibilityToggle
# Go to points
bindsym Mode_switch+f mode "mode_mouse_move"; $mouseHintsVisibilityToggle

# Font for window titles. Will also be used by the bar unless a different font
# is used in the bar {} block below.
font pango:monospace 8

# This font is widely installed, provides lots of unicode glyphs, right-to-left
# text rendering and scalability on retina/hidpi displays (thanks to pango).
#font pango:DejaVu Sans Mono 8

# Before i3 v4.8, we used to recommend this one as the default:
# font -misc-fixed-medium-r-normal--13-120-75-75-C-70-iso10646-1
# The font above is very space-efficient, that is, it looks good, sharp and
# clear in small sizes. However, its unicode glyph coverage is limited, the old
# X core fonts rendering does not support right-to-left and this being a bitmap
# font, it doesnt scale on retina/hidpi displays.

# Use Mouse+$mod to drag floating windows to their wanted position
floating_modifier $mod

# start a terminal
bindsym control+Mod1+t exec i3-sensible-terminal 

# start a chrome window
bindsym $sup+Return exec chromium-browser 
bindsym $sup+shift+Return exec chromium-browser --incognito
bindsym $mod+Mod1+Return exec quteb

bindsym $mod+Return exec i3-sensible-terminal

# kill focused window
bindsym $mod+Shift+q kill

# start dmenu (a program launcher)
# bindsym $mod+d exec dmenu_run -nb black -sb tomato -l 10
# bindsym $mod+d exec rofi -show run -lines 7 -eh 2 -bg "black" -fg "#B00000" -font "System San Francisco Display 15" -hlfg "#999999"
bindsym $mod+d $rofi
bindsym $mod+shift+d $rofiWindow

# bindsym $mod+Mod1+d exec rofi

