bindsym $sup+j exec xdotool click 1
bindsym $sup+Shift+j exec xdotool click 3

set $mode_small_mouse_move
mode "mode_small_mouse_move" {
    bindsym h exec xdotool mousemove_relative --sync -- -10 0
    bindsym j exec xdotool mousemove_relative --sync 0 10
    bindsym k exec xdotool mousemove_relative --sync 0 -10
    bindsym l exec xdotool mousemove_relative --sync 10 0 

    bindsym $mod+h exec xdotool mousemove_relative --sync -- -25 0
    bindsym $mod+j exec xdotool mousemove_relative --sync 0 25
    bindsym $mod+k exec xdotool mousemove_relative --sync 0 -25
    bindsym $mod+l exec xdotool mousemove_relative --sync 25 0 

    bindsym Shift+h exec xdotool mousemove_relative --sync -- -75 0
    bindsym Shift+j exec xdotool mousemove_relative --sync 0 50
    bindsym Shift+k exec xdotool mousemove_relative --sync 0 -50
    bindsym Shift+l exec xdotool mousemove_relative --sync 50 0 
    
    bindsym $mod+Shift+h exec xdotool mousemove_relative --sync -- -100 0
    bindsym $mod+Shift+j exec xdotool mousemove_relative --sync 0 100
    bindsym $mod+Shift+k exec xdotool mousemove_relative --sync 0 -100
    bindsym $mod+Shift+l exec xdotool mousemove_relative --sync 100 0 

    bindsym Mod1+h exec xdotool mousemove_relative --sync -- -300 0
    bindsym Mod1+j exec xdotool mousemove_relative --sync 0 300
    bindsym Mod1+k exec xdotool mousemove_relative --sync 0 -300
    bindsym Mod1+l exec xdotool mousemove_relative --sync 300 0 

    # # prev mode
    # bindsym Shift+space mode "mode_mouse_move"; exec xdotool keydown super keyup super && xdotool mouseup 1
    bindsym apostrophe mode "mode_mouse_move"
    bindsym $mod+space mode "mode_mouse_move"

    # next mode
    # bindsym space mode "mode_click"; exec xdotool keydown super keyup super && xdotool mouseup 1

    bindsym Mod1+j exec xdotool click 4
    bindsym Mod1+k exec xdotool click 5

    # exit mode
    bindsym $sup+space mode "default"; exec xdotool keydown super keyup super && xdotool mouseup 1 && gromit-mpx -v
    bindsym Escape mode "default"; exec xdotool keydown super keyup super && xdotool mouseup 1 && gromit-mpx -v

    # Left click and exit
    bindsym space mode "default"; exec xdotool click 1 && xdotool mouseup 1 && gromit-mpx -v
    # Right click and exit
    bindsym Shift+space mode "default"; exec xdotool click 3 && xdotool mouseup 1 && gromit-mpx -v

    # Middle Mouse and exit
    bindsym Return mode "default"; exec xdotool click 2 && xdotool mouseup 1 && gromit-mpx -v

    # Hold down
    bindsym Mod1+space exec xdotool mousedown 1 && i3-msg mode "mode_mouse_move" && xdotool mousemove_relative --sync 5 5

    # Release
    bindsym Mod1+Shift+space exec xdotool mouseup 1

    # All combos except for h, j, k, and l
    bindsym 1 exec python $SCRIPTS/mouse_jumper.py 0 0 && $SCRIPTS/i3_mouse_move_additional_action.sh
    bindsym $mod+1 exec python $SCRIPTS/mouse_jumper.py 0 1 && $SCRIPTS/i3_mouse_move_additional_action.sh
    bindsym $mod+shift+1 exec python $SCRIPTS/mouse_jumper.py 0 2 && $SCRIPTS/i3_mouse_move_additional_action.sh
    bindsym shift+1 exec python $SCRIPTS/mouse_jumper.py 0 3 && $SCRIPTS/i3_mouse_move_additional_action.sh
    bindsym Mod1+1 exec python $SCRIPTS/mouse_jumper.py 1 0 && $SCRIPTS/i3_mouse_move_additional_action.sh
    bindsym Mod1+$mod+1 exec python $SCRIPTS/mouse_jumper.py 1 1 && $SCRIPTS/i3_mouse_move_additional_action.sh
    bindsym Mod1+$mod+shift+1 exec python $SCRIPTS/mouse_jumper.py 1 2 && $SCRIPTS/i3_mouse_move_additional_action.sh
    bindsym Mod1+shift+1 exec python $SCRIPTS/mouse_jumper.py 1 3 && $SCRIPTS/i3_mouse_move_additional_action.sh

    bindsym 2 exec python $SCRIPTS/mouse_jumper.py 2 0 && $SCRIPTS/i3_mouse_move_additional_action.sh
    bindsym $mod+2 exec python $SCRIPTS/mouse_jumper.py 2 1 && $SCRIPTS/i3_mouse_move_additional_action.sh
    bindsym $mod+shift+2 exec python $SCRIPTS/mouse_jumper.py 2 2 && $SCRIPTS/i3_mouse_move_additional_action.sh
    bindsym shift+2 exec python $SCRIPTS/mouse_jumper.py 2 3 && $SCRIPTS/i3_mouse_move_additional_action.sh
    bindsym Mod1+2 exec python $SCRIPTS/mouse_jumper.py 3 0 && $SCRIPTS/i3_mouse_move_additional_action.sh
    bindsym Mod1+$mod+2 exec python $SCRIPTS/mouse_jumper.py 3 1 && $SCRIPTS/i3_mouse_move_additional_action.sh
    bindsym Mod1+$mod+shift+2 exec python $SCRIPTS/mouse_jumper.py 3 2 && $SCRIPTS/i3_mouse_move_additional_action.sh
    bindsym Mod1+shift+2 exec python $SCRIPTS/mouse_jumper.py 3 3 && $SCRIPTS/i3_mouse_move_additional_action.sh

